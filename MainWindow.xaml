<Window x:Class="GraphBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphBuilder"
        mc:Ignorable="d"
        Title="MainWindow" Height="820" Width="1920" Background="#FF1B3148" WindowState="Maximized">
    <Grid x:Name="MainGrid">
        <Rectangle Height="100" Fill="#FF1B3148" Grid.Column="1" Margin="10,770,0,-71" Panel.ZIndex="1000"/>
        <Border Grid.Column="0" Background="#FF455E83" CornerRadius="3" Margin="30, 30,0, 30" BorderBrush="White" BorderThickness="1"/>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="4*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
    </Grid>
</Window>
    
